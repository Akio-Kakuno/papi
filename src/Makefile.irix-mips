SUBSTR  = irix-mips
DESCR	= "IRIX 6.x or greater with MIPS R10000 or R12000"
LIBS	= static shared
TARGETS	= serial smp omp
FLAGS  	= 
CFLAGS  = -woff 1116,1552 $(FLAGS)
NOOPT	= -O0
OPTFLAGS= -O3
FFLAGS	= $(FLAGS)
SMPCFLGS= -mp
OMPCFLGS= -mp
CC_SHR  = cc -shared -all -check_registry /usr/lib/so_locations -rpath $(PWD) -soname libpapi.so
CC_R	= cc
CC	= cc
CPP	= cc -E
F77	= f77

include ./Makefile.inc

